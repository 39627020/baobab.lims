<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="bika">

    <adapter
      factory="bika.sanbi.browser.inventory.SupplierContactVocabularyFactory"
      provides="bika.lims.interfaces.IDisplayListVocabulary"
      for="bika.sanbi.interfaces.IInventoryOrder"
      name="getContacts"
    />

    <adapter
      for="bika.sanbi.interfaces.IInventoryOrder"
      factory="bika.sanbi.browser.inventory.mailto_link_from_contacts"
      provides="bika.lims.interfaces.IHeaderTableFieldRenderer"
      name="Contact"
    />
    <adapter
      for="bika.sanbi.interfaces.IInventoryOrder"
      factory="bika.sanbi.browser.inventory.mailto_link_from_contacts"
      provides="bika.lims.interfaces.IHeaderTableFieldRenderer"
      name="CContact"
    />

    <adapter
      for="bika.sanbi.interfaces.IInventoryOrder"
      factory="bika.sanbi.browser.inventory.mailto_link_from_ccemails"
      provides="bika.lims.interfaces.IHeaderTableFieldRenderer"
      name="CCEmails"
    />

    <!-- main site/order view -->
    <browser:page
      for="bika.sanbi.interfaces.IInventoryOrderFolder"
      name="base_view"
      class="bika.sanbi.browser.inventory.orderfolder.OrderFolderView"
      permission="zope2.View"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
      for="bika.sanbi.interfaces.IInventoryOrder"
      name="base_edit"
      class="bika.sanbi.browser.inventory.order.EditView"
      permission="cmf.ModifyPortalContent"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
      for="bika.sanbi.interfaces.IInventoryOrder"
      name="base_view"
      class="bika.sanbi.browser.inventory.order.OrderView"
      permission="cmf.ModifyPortalContent"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
      for="bika.sanbi.interfaces.IInventoryOrder"
      name="print"
      class="bika.sanbi.browser.inventory.order.PrintView"
      permission="cmf.ModifyPortalContent"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
      for="bika.sanbi.interfaces.IInventoryOrder"
      name="publish"
      class="bika.sanbi.browser.inventory.publish.OrderPublishView"
      permission="bika.lims.ManageBika"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <!-- Translated breadcrumbs -->
    <browser:viewlet
      for="bika.sanbi.interfaces.IInventoryOrder"
      name="plone.path_bar"
      manager="plone.app.layout.viewlets.interfaces.IAboveContent"
      template="templates/path_bar.pt"
      class="bika.sanbi.browser.inventory.order.OrderPathBarViewlet"
      permission="zope2.View"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
     for="bika.sanbi.interfaces.IInventoryOrder"
     name="stickers"
     class="bika.sanbi.browser.inventory.stickers.Sticker"
     permission="zope2.View"
     layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
      for="bika.sanbi.interfaces.IInventoryOrder"
      name="store"
      class="bika.sanbi.browser.inventory.order.OrderStore"
      permission="cmf.ModifyPortalContent"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
      for="bika.sanbi.interfaces.IProduct"
      name="multifile"
      class=".product.ProductMultifileView"
      permission="zope.Public"
      layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
        for="bika.sanbi.interfaces.IProduct"
        name="base_view"
        class=".product.ProductStorageView"
        permission="zope.Public"
        layer="bika.lims.interfaces.IBikaLIMS"
    />

    <browser:page
        for="bika.sanbi.interfaces.IProduct"
        name="productqtty"
        class=".product.ProductQuantityByStorage"
        permission="zope.Public"
        layer="bika.lims.interfaces.IBikaLIMS"
    />

</configure>
